{
  "name": "needier",
  "version": "0.4.0",
  "description": "Dependency ordering",
  "main": "lib",
  "author": "Eli Doran <eli@elidoran.com>",
  "license": "MIT",
  "scripts": {
    "compile": "coffee -c lib",
    "clean": "rm -rf ./lib/*.js",
    "prepublish": "coffee --bare --compile lib",
    "postpublish": "rm -rf ./lib/*.js",
    "test": "mocha --compilers coffee:coffee-script/register --reporter spec --bail --check-leaks test/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/elidoran/needier.git"
  },
  "bugs": "http://github.com/elidoran/needier/issues",
  "devDependencies": {
    "coffee-script": "^1.9.2",
    "mocha": "^2.2.4"
  },
  "dependencies": {
    "had": "^0.6.0"
  }
}
